    <ItemGroup>
        <PackageReference Include="Nuke.Common" Version="6.2.1" />
        <PackageDownload Include="NuGet.CommandLine" Version="[5.9.1]" />
        <PackageDownload Include="GitVersion.Tool" Version="[5.10.3]" />
        <PackageDownload Include="Umbraco.Tools.Packages" Version="[1.0.1]" />
    </ItemGroup>

</Project>
